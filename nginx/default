server {
    listen 80;

    server_name _;
    
    root /usr/share/nginx/html;

    index index.html index.htm index.php;

    charset utf-8;

    location / {
    	
    }

    access_log off;
    # error_log  /var/log/nginx/error.log error;

    location ~ /\.ht {
        deny all;
    }
}
                                                                                
